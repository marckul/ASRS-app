nts.scss","name":null,"original":{"line":154,"column":16},"generated":{"line":199,"column":27}},{"source":"..\\src\\styles\\_components.scss","name":null,"original":{"line":155,"column":6},"generated":{"line":200,"column":2}},{"source":"..\\src\\styles\\_components.scss","name":null,"original":{"line":155,"column":18},"generated":{"line":200,"column":31}},{"source":"..\\src\\styles\\_components.scss","name":null,"original":{"line":144,"column":0},"generated":{"line":205,"column":2}},{"source":"..\\src\\styles\\_components.scss","name":null,"original":{"line":144,"column":17},"generated":{"line":205,"column":19}},{"source":"..\\src\\styles\\_components.scss","name":null,"original":{"line":164,"column":0},"generated":{"line":209,"column":2}},{"source":"..\\src\\styles\\_components.scss","name":null,"original":{"line":164,"column":5},"generated":{"line":209,"column":21}}],"sources":{"..\\src\\styles\\main\\main.scss":"\r\n// @import url('https://fonts.googleapis.com/css2?family=Lato:ital,wght@0,300;0,400;0,700;0,900;1,300;1,400;1,700;1,900&display=swap');\r\n@import url('https://fonts.googleapis.com/css2?family=Lato:ital,wght@0,900;1,900&display=swap');\r\n\r\n\r\n$markul-color-primaryLight: #ADBBE1;\r\n$markul-color-primary: #336CFB;\r\n\r\n$markul-color-primaryDark: #2E3A59;\r\n$markul-color-primaryDark-hover: ($markul-color-primary + $markul-color-primaryDark) / 2;\r\n$markul-color-primaryDarker: #0E111B;\r\n\r\n$markul-color-secondary: #86A8FF;\r\n\r\n$markul-color-navActive: #0F618A;\r\n$markul-color-navActiveLight: #7BCFE1;\r\n$markul-color-radioActive: #86A8FF;\r\n\r\n$markul-color-snow: #F2F7FF;\r\n$markul-color-grey: #ECF3FD;\r\n$markul-color-activeLight: #7BCFE1;\r\n\r\n\r\n$markul-color-logo: $markul-color-navActive;\r\n\r\n@import '../variables';\r\n\r\n@import '../../custom';\r\n\r\n\r\n\r\n.my-icon {\r\n  color: inherit;\r\n  position: relative;\r\n  display: inline-block;\r\n  min-width: 1.2em;\r\n  min-height: 1.2em;\r\n  top: -.15em;\r\n  // bottom: 1em;\r\n}\r\n\r\n.icon-happy {\r\n  top: .25em;\r\n  background-position: center;\r\n  // background-attachment: fixed;\r\n  background-repeat: no-repeat;\r\n  background-size: contain;\r\n  background-image: url(\"../../assets/icons/happy.svg\");\r\n}\r\n\r\n\r\n\r\n\r\n/* =============\r\n  MAIN STYLES\r\n=============== */\r\n\r\n.border-white {\r\n  border-color: white !important;\r\n}\r\n\r\n\r\n#asrs-logo {\r\n  color: $markul-color-logo;\r\n  min-height: 10rem;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n\r\n\r\n/* =============\r\n  TEXT\r\n=============== */\r\n\r\nbody {\r\n  font-family: 'Lato', sans-serif !important;\r\n  color: $markul-color-primaryDark;\r\n}\r\n\r\nh1, h2, h3, h4, h5, h6 {\r\n  font-family: 'Lato', sans-serif !important;\r\n  font-weight: 700;\r\n}\r\n\r\n.navbar-nav {\r\n  font-weight: 700 !important;  \r\n}\r\n\r\n@import '../text_styles';\r\n\r\n/* =============\r\n  LAYOUT\r\n=============== */\r\nsection {\r\n  padding-top: 30px;\r\n  margin-top: -30px;\r\n}\r\n\r\nbody {\r\n  position: relative;\r\n}\r\n@import '../components';\r\n\r\n","..\\src\\_custom.scss":"// ZAWIERA SCSS BOOTSTRAPA\r\n\r\n// Custom.scss\r\n// Option B: Include parts of Bootstrap\r\n\r\n// // 1. Include functions first (so you can manipulate colors, SVGs, calc, etc)\r\n// @import \"../node_modules/bootstrap/scss/functions\";\r\n\r\n// // 2. Include any default variable overrides here\r\n\r\n// // 3. Include remainder of required Bootstrap stylesheets\r\n// @import \"../node_modules/bootstrap/scss/variables\";\r\n// @import \"../node_modules/bootstrap/scss/mixins\";\r\n\r\n// // 4. Include any optional Bootstrap components as you like\r\n// @import \"../node_modules/bootstrap/scss/root\";\r\n// @import \"../node_modules/bootstrap/scss/reboot\";\r\n// @import \"../node_modules/bootstrap/scss/type\";\r\n// @import \"../node_modules/bootstrap/scss/images\";\r\n// @import \"../node_modules/bootstrap/scss/containers\";\r\n// @import \"../node_modules/bootstrap/scss/grid\";\r\n\r\n// @import \"../node_modules/bootstrap/scss/scss/navbar\";\r\n\r\n\r\n// @import \"../node_modules/bootstrap/dist/css/bootstrap.min.css\";\r\n\r\n// @import '../node_modules/bootstrap/scss/bootstrap';\r\n\r\n// 5. Add additional custom code here\r\n\r\n\r\n// body {\r\n//   color: red;\r\n// }\r\n\r\n\r\n.navbar-light {\r\n  border-bottom: solid .1rem $markul-color-secondary;\r\n}\r\n\r\n\r\n.bg-dark {\r\n  // background-color: $markul-color-primaryDark !important;\r\n}\r\n\r\n.navbar-dark .navbar-nav .nav-link {\r\n  color: $markul-color-secondary;\r\n}\r\n\r\n$markul-color-hover: ($markul-color-activeLight + #ffffff)/2;\r\n.navbar-dark .navbar-nav .nav-link:focus, .navbar-dark .navbar-nav .nav-link:hover {\r\n  // color: ($markul-color-secondary * 2 + #ffffff)/3;\r\n  color: $markul-color-hover;\r\n}\r\n\r\n.navbar-dark .navbar-nav .nav-link.active, .navbar-dark .navbar-nav .show>.nav-link {\r\n  color: $markul-color-activeLight;\r\n  &:focus, &:hover {\r\n    color: $markul-color-hover;\r\n  }\r\n}\r\n\r\n// .navbar-dark .navbar-nav .nav-link.active:focus, .navbar-dark .navbar-nav .show>.nav-link:focus {\r\n//   color: ($markul-color-activeLight + #ffffff)/2;\r\n// }\r\n","..\\src\\styles\\_components.scss":"\r\n$markul-color-sidebar: white;// $markul-color-grey;// #edf1f5;//\r\n.sidebar {\r\n  position: fixed;\r\n  z-index: 100;\r\n  top: 0;\r\n  left: 0;\r\n  bottom: 0;\r\n  background-color: $markul-color-sidebar;\r\n  // border-right: solid .1rem $markul-color-primaryLight;\r\n  padding-top: 56px;\r\n  .accordion-item, .accordion-header, .accordion-button {\r\n    background: transparent !important;\r\n    color: inherit;\r\n  }\r\n  .accordion-button:not(.collapsed) {\r\n    box-shadow: none !important;\r\n  }\r\n}\r\n\r\n\r\n#test-properties {\r\n  background-color: transparent;\r\n  .title {\r\n    color: $markul-color-primaryLight;\r\n    margin-top: 3rem;\r\n    margin-bottom: 1.5rem;\r\n    text-align: center;\r\n    font-weight: normal;\r\n    font-style: italic;\r\n    text-transform: uppercase;\r\n    font-size: 1em;\r\n  }\r\n\r\n  .accordion-button, h4 {\r\n    &:hover {\r\n      color: $markul-color-primaryDark-hover;\r\n    }\r\n  }\r\n  \r\n}\r\n\r\n\r\n.main-content {\r\n  padding: 0 1rem;    \r\n  @media screen and (min-width: 1000px) {\r\n    padding: 0 2rem;    \r\n  } \r\n}\r\n\r\n\r\n.results-container {\r\n  min-height: 75vh;\r\n}\r\n\r\n\r\n/* =================\r\n  FORMULARZ\r\n==================== */ \r\n\r\n.asrs-form-list {\r\n  li {\r\n    font-style: italic;\r\n  }\r\n}\r\n\r\n#main-content {\r\n  .asrs-form-row {\r\n    font-style: italic;\r\n    margin-bottom: .5em;\r\n    p {\r\n      margin-bottom: 0;\r\n    }\r\n  }\r\n\r\n  \r\n  \r\n}\r\n\r\n\r\n.asrs-radio-group {\r\n  font-size: 1.1rem;\r\n  display: flex;\r\n  flex-wrap: nowrap;\r\n  max-width: max-content;\r\n  margin-left: auto;\r\n  // ..................\r\n  // WERSJA 1\r\n  // ..................\r\n  // .my-radio-button {\r\n  //   display: flex;\r\n  //   flex-direction: row;\r\n  //   justify-content: center;\r\n  //   margin-left: .8em;\r\n  //   input {\r\n  //     width: 1.25rem;\r\n  //     height: 1.25rem;\r\n  //   }      \r\n  //   label {\r\n  //     margin-left: .25em;\r\n  //   }\r\n  // }\r\n}\r\n\r\n// ..................\r\n// WERSJA 2\r\n// ..................\r\n\r\n\r\n.my-radio-button {\r\n  display: inline-block;\r\n  position: relative;\r\n  padding: 0 6px;\r\n  // margin: 10px 0 0;\r\n}\r\n\r\n\r\n// SINGLE RADIO BUTTON\r\n$color-radioButtonChecked:  rgb(48, 168, 0);  // $markul-color-secondary\r\n$color-labelButtonChecked: rgb(27, 94, 0); \r\n.my-radio-button input[type='radio'] {\r\n  display: none;\r\n}\r\n\r\n\r\n.my-radio-button label {\r\n  color: $markul-color-primaryDark;\r\n  font-weight: normal;\r\n}\r\n\r\n.my-radio-button label:before {\r\n  content: \" \";\r\n  display: inline-block;\r\n  position: relative;\r\n  top: 5px;\r\n  margin: 0 5px 0 0;\r\n  width: 20px;\r\n  height: 20px;\r\n  border-radius: 11px;\r\n  border: 2px solid  $markul-color-primaryDark;\r\n  background-color: transparent;\r\n}\r\n\r\n.my-radio-button input:checked { // [type=radio]\r\n  + label {\r\n    color: $color-labelButtonChecked;\r\n    font-weight: bold;\r\n    &:after {\r\n      border-radius: 11px;\r\n      width: 12px;\r\n      height: 12px;\r\n      position: absolute;\r\n      top: 9px;\r\n      left: 10px;\r\n      content: \" \";\r\n      display: block;\r\n      background: $color-radioButtonChecked;\r\n    }    \r\n    &:before {\r\n      border: 2px solid $color-radioButtonChecked;\r\n    }\r\n  }  \r\n}\r\n/* */ \r\n\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"42b0ca9a12a6f3ecbe6f4e4eb59c9241","cacheData":{"env":{}}}